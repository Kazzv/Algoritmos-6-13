#include <stdio.h>

int main(void) {
   int num, sumaTotal;
sumaTotal = 0;
    
    printf ("Ingrese un numero: \n");
    scanf ("%d", &num);

    while (num >= 0) {
        if (num >= 0) {
            sumaTotal = sumaTotal + num;
            printf ("Ingrese otro numero: \n");
        scanf ("%d", &num);
        }
    }

    printf ("La sumatoria de todos los numeros ingresados es: %d", sumaTotal);
    return 0;
}
